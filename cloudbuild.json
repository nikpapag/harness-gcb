{
    "steps": [
    {
        "name": "gcr.io/cloud-builders/git",
        "args": ["clone", "https://github.com/nikpapag/harness-gcb.git", "."]
    },
    {
        "name": "gcr.io/cloud-builders/docker",
        "args": [
            "build",
            "-t",
            "us-west2-docker.pkg.dev/sales-209522/quickstart-docker-repo-nikp/quickstart-image:tag1",
            "."
        ]
    },
    {
        "name: "gcr.io/cloud-builders/gcloud",
        "entrypoint": "/bin/bash",
        "env": "export $OUTPUT=us-west2-docker.pkg.dev/sales-209522/quickstart-docker-repo-nikp/quickstart-image:tag1"
        
            
    ]
}
